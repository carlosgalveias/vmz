    "eslint-config-integromat": "^1.5.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "globals": "^15.9.0",
    "mocha": "^10.7.3"
  },
  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "test": "mocha test",
    "pretest": "eslint ."
  },
  "bin": {
    "vmz": "./bin/vmz"
